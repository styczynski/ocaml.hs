{
  "name": "@ocamlhs/web-frontend",
  "version": "2.1.2",
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "parser": "babylon",
    "printWidth": 80,
    "proseWrap": "preserve",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "dependencies": {
    "babylon": "^6.18.0",
    "babylon-walk": "^1.0.2",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.2.0",
    "lodash": "^4.17.15",
    "preact": "^8.5.1",
    "prop-types": "^15.7.2",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4",
    "whatwg-fetch": "3.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@remy/eslint": "^3.2.2",
    "autoprefixer": "9.6.1",
    "babel-cli": "^6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "8.0.6",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^9.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "6.26.0",
    "body-parser": "^1.19.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "chalk": "^2.4.2",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "3.2.0",
    "dotenv": "8.0.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-react-app": "^5.0.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-flowtype": "^4.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.15.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "4.2.0",
    "fs-extra": "8.1.0",
    "html-webpack-include-assets-plugin": "^2.0.0",
    "html-webpack-plugin": "^4.0.0-beta.8",
    "jest": "^24.8.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-uuid": "^1.4.8",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "preact-compat": "^3.19.0",
    "prettier": "1.18.2",
    "prettier-eslint": "^9.0.0",
    "promise": "8.0.3",
    "react": "^16.9.0",
    "react-dev-utils": "^9.0.3",
    "react-error-overlay": "^6.0.1",
    "react-test-renderer": "^16.9.0",
    "redbox-react": "^1.6.0",
    "redux-devtools": "^3.5.0",
    "style-loader": "1.0.0",
    "sw-precache-webpack-plugin": "0.11.5",
    "url-loader": "2.1.0",
    "webpack": "4.39.2",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-concat-plugin": "^3.0.0",
    "webpack-dev-server": "3.8.0",
    "webpack-manifest-plugin": "2.0.4"
  },
  "_proxy": "http://localhost:3001",
  "scripts": {
    "env": "env",
    "start": "npm run styles && REACT_APP_VERSION=$npm_package_version node scripts/start.js",
    "build": "rm -rf build; npm run styles; PUBLIC_URL=\"http://styczynski.in/ocaml.hs/\" REACT_APP_VERSION=$npm_package_version node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "styles": "cat src/*.css > src/core/jsconsole.css",
    "watch": "nodemon -w src/ -e css -x 'npm run styles'",
    "lib": "rm -rf dist; npm run styles; NODE_ENV=production babel src/core --out-dir dist --copy-files --ignore '__tests__'",
    "deploy": "npm run build && s3_website push",
    "prepare": "npm run lib"
  },
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "jest": {
    "modulePathIgnorePatterns": [
      "object-assign"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "<rootDir>[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx"
    ]
  },
  "babel": {
    "presets": [
      "@babel/react",
      "@babel/env"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties"
      ]
    ]
  },
  "__eslintConfig": {
    "extends": "react-app"
  },
  "description": "Web based console - for presentations, workshops and jsbin",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/remy/jsconsole.git"
  },
  "keywords": [],
  "author": "Remy Sharp <remy@leftlogic.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/remy/jsconsole/issues"
  },
  "homepage": "https://styczynski.in/ocaml.hs/"
}
